{
  "name": "@howtographql/gatsby-theme",
  "version": "1.0.1",
  "description": "The content/gatsby theme for htg2",
  "main": "index.ts",
  "license": "MIT",
  "private": false,
  "scripts": {
    "generate": "graphql-codegen"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.1.1",
    "@graphql-codegen/typescript": "^1.1.1",
    "@graphql-codegen/typescript-operations": "^1.1.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.7",
    "@types/react-helmet": "^5.0.8",
    "@types/rebass__grid": "^6.0.4",
    "@types/styled-components": "^4.1.12",
    "@types/typography": "^0.16.2",
    "gatsby": "^2.3.24",
    "gatsby-plugin-root-import": "^2.0.5",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "loader-utils": "^1.2.3",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "gatsby": "^2.3.24",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@mdx-js/loader": "^0.20.3",
    "@mdx-js/mdx": "^1.0.0-rc.0",
    "@mdx-js/react": "^1.0.0-rc.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-fetch": "^0.7.0",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby-image": "^2.0.33",
    "gatsby-mdx": "^0.5.7",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-page-creator": "^2.0.12",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sharp": "^2.0.28",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.17",
    "isomorphic-fetch": "^2.2.1",
    "mdx-utils": "^0.1.1",
    "prism-react-renderer": "^0.1.6",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react-apollo": "^2.5.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-helmet": "^5.2.0",
    "react-live": "^2.0.1",
    "react-typography": "^0.16.19",
    "styled-components": "^4.1.3",
    "styled-system": "^4.1.0",
    "typography": "^0.16.19"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
